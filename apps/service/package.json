{
  "name": "@apps/service",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=20.19.2"
  },
  "scripts": {
    "clear:modules": "if exist node_modules (rmdir /s /q node_modules)",
    "clear:dist": "if exist dist (rmdir /s /q dist)",
    "clear": "pnpm clear:dist && pnpm clear:modules",
    "build": "nest build",
    "format:fix": "prettier --write \"{src,apps,libs}/**/*.{ts,cjs,mjs,js,ejs,md,html,css,md,json}\"",
    "format": "prettier --check \"{src,apps,libs}/**/*.{ts,cjs,mjs,js,ejs,md,html,css,md,json}\"",
    "eslint:fix": "eslint \"{src,apps,libs}/**/*.{ts,cjs,mjs,js}\" --fix",
    "eslint": "eslint \"{src,apps,libs}/**/*.{ts,cjs,mjs,js}\"",
    "lint:fix": "pnpm format:fix && pnpm eslint:fix",
    "lint": "pnpm format && pnpm eslint",
    "start": "nest start",
    "dev": "cross-env ENV_NAME=dev nest build --webpack --webpackPath webpack-hmr.config.cjs --watch",
    "pm2:start": "cross-env pm2_home=.pm2 pm2 start ecosystem.config.js",
    "pm2:stop": "cross-env pm2_home=.pm2 pm2 stop ecosystem.config.js",
    "pm2:restart": "cross-env pm2_home=.pm2 pm2 restart ecosystem.config.js",
    "pm2:reload": "cross-env pm2_home=.pm2 pm2 reload ecosystem.config.js",
    "pm2:delete": "cross-env pm2_home=.pm2 pm2 delete ecosystem.config.js",
    "pm2:list": "cross-env pm2_home=.pm2 pm2 list",
    "pm2:logs": "cross-env pm2_home=.pm2 pm2 logs",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nest-lab/throttler-storage-redis": "catalog:api_prod",
    "@nestjs-modules/mailer": "catalog:api_prod",
    "@nestjs/axios": "catalog:api_prod",
    "@nestjs/bullmq": "catalog:api_prod",
    "@nestjs/cache-manager": "catalog:api_prod",
    "@nestjs/common": "catalog:api_prod",
    "@nestjs/config": "catalog:api_prod",
    "@nestjs/core": "catalog:api_prod",
    "@nestjs/event-emitter": "catalog:api_prod",
    "@nestjs/jwt": "catalog:api_prod",
    "@nestjs/mapped-types": "catalog:api_prod",
    "@nestjs/mongoose": "catalog:api_prod",
    "@nestjs/passport": "catalog:api_prod",
    "@nestjs/platform-express": "catalog:api_prod",
    "@nestjs/schedule": "catalog:api_prod",
    "@nestjs/swagger": "catalog:api_prod",
    "@nestjs/terminus": "catalog:api_prod",
    "@nestjs/throttler": "catalog:api_prod",
    "@nestjs/typeorm": "catalog:api_prod",
    "@packages/types": "workspace:*",
    "axios": "catalog:cli",
    "bullmq": "catalog:api_prod",
    "cache-manager": "catalog:api_prod",
    "chalk": "catalog:api_prod",
    "class-transformer": "catalog:api_prod",
    "class-validator": "catalog:api_prod",
    "cookie-parser": "catalog:api_prod",
    "cross-env": "catalog:api_prod",
    "crypto-js": "catalog:cli",
    "csrf-csrf": "catalog:api_prod",
    "dayjs": "catalog:cli",
    "ejs": "catalog:api_prod",
    "etag": "catalog:api_prod",
    "helmet": "catalog:api_prod",
    "ioredis": "catalog:api_prod",
    "joi": "catalog:api_prod",
    "lodash-es": "catalog:cli",
    "mongoose": "catalog:api_prod",
    "mysql2": "catalog:api_prod",
    "nestjs-cls": "catalog:api_prod",
    "nodemailer": "catalog:api_prod",
    "passport": "catalog:api_prod",
    "passport-jwt": "catalog:api_prod",
    "passport-local": "catalog:api_prod",
    "pm2": "catalog:api_prod",
    "reflect-metadata": "catalog:api_prod",
    "rxjs": "catalog:api_prod",
    "svg-captcha": "catalog:api_prod",
    "typeorm": "catalog:api_prod",
    "uuid": "catalog:cli",
    "winston": "catalog:api_prod",
    "winston-daily-rotate-file": "catalog:api_prod",
    "winston-mongodb": "catalog:api_prod"
  },
  "devDependencies": {
    "@configs/eslint-config": "workspace:*",
    "@nestjs/cli": "catalog:api_dev",
    "@nestjs/schematics": "catalog:api_dev",
    "@nestjs/testing": "catalog:api_dev",
    "@types/cookie-parser": "catalog:api_dev",
    "@types/crypto-js": "catalog:cli",
    "@types/ejs": "catalog:api_dev",
    "@types/etag": "catalog:api_dev",
    "@types/express": "catalog:api_dev",
    "@types/jest": "catalog:api_dev",
    "@types/lodash-es": "catalog:cli",
    "@types/node": "catalog:cli",
    "@types/nodemailer": "catalog:api_dev",
    "@types/passport-jwt": "catalog:api_dev",
    "@types/passport-local": "catalog:api_dev",
    "@types/supertest": "catalog:api_dev",
    "copy-webpack-plugin": "catalog:api_dev",
    "eslint": "catalog:cli",
    "globals": "catalog:api_dev",
    "jest": "catalog:api_dev",
    "nestjs-spelunker": "catalog:api_dev",
    "run-script-webpack-plugin": "catalog:api_dev",
    "source-map-support": "catalog:api_dev",
    "supertest": "catalog:api_dev",
    "ts-jest": "catalog:api_dev",
    "ts-loader": "catalog:api_dev",
    "ts-node": "catalog:api_dev",
    "tsconfig-paths": "catalog:api_dev",
    "typescript": "catalog:cli",
    "webpack": "catalog:api_dev",
    "webpack-bundle-analyzer": "catalog:api_dev",
    "webpack-node-externals": "catalog:api_dev"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
